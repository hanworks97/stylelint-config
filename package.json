{
  "name": "@hanworks/stylelint-config",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@10.15.0",
  "description": "Hanworks Stylelint config",
  "author": "Hanworks <khan97z.miao@gmail.com> (https://github.com/hanworks97/)",
  "license": "MIT",
  "homepage": "https://github.com/hanworks97/stylelint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanworks97/stylelint-config.git"
  },
  "bugs": {
    "url": "https://github.com/hanworks97/stylelint-config/issues"
  },
  "keywords": [
    "stylelint-config"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "postinstall": "npm run build",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "peerDependencies": {
    "autoprefixer": ">=10.4.20",
    "postcss": ">=8.5.3",
    "stylelint": ">=16.14.1"
  },
  "dependencies": {
    "@stylistic/stylelint-plugin": "^3.1.2",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.11.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.15.0",
    "@hanworks/eslint-config": "^1.0.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.29.0",
    "eslint-plugin-format": "^1.0.2",
    "lint-staged": "^16.2.7",
    "rollup": "^4.34.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "simple-git-hooks": "^2.13.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
